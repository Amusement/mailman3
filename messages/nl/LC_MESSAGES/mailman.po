"    <TR>\n"
"      <TD COLSPAN=\"2\" WIDTH=\"100%%\" BGCOLOR=\"#99CCFF\" ALIGN=\"CENTER"
"\">\n"
"\t<B><FONT COLOR=\"#000000\" SIZE=\"+1\">%(realname)s Private\n"
"\t    Archives Authentication</FONT></B>\n"
"      </TD>\n"
"    </TR>\n"
"    <tr>\n"
